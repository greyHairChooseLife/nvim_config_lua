{
  "coc.preferences.formatOnSave": true,
  "prettier.prettierPath": "./node_modules/prettier",
  "flutter.lsp.initialization.closingLabels": false,
  "markdownlint.onChange": true,
  "sumneko-lua.enableNvimLuaDev": false,

  "coc.source.css.filetypes": ["css", "scss", "less"],
  "coc.source.docker.filetypes": ["dockerfile"],
  "coc.source.flutter.filetypes": ["dart"],
  "coc.source.html.filetypes": ["html"],
  "coc.source.json.filetypes": ["json"],
  "coc.source.pairs.filetypes": [],
  "coc.source.prettier.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "css",
    "scss",
    "less",
    "html",
    "json",
    "markdown"
  ],
  "coc.source.sh.filetypes": ["sh", "bash"],
  "coc.source.sumneko-lua.filetypes": ["lua"],
  "coc.source.tsserver.filetypes": [
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact"
  ],
  "coc.source.word.filetypes": ["markdown", "text"],
  "coc.source.yaml.filetypes": ["yaml", "yml"],
  "coc.source.python.filetypes": ["python"],
  "coc.source.vim-dadbod-completion": {
    "filetypes": ["sql"],
    "priority": 9,
    "shortcut": "DB"
  },

  "pyright.inlayHints.functionReturnTypes": false,
  "pyright.inlayHints.variableTypes": false,
  "pyright.inlayHints.parameterTypes": false,

  "pairs.disableLanguages": ["vimwiki", "markdown", "TelescopePrompt"],
  "pretty-ts-errors.codeBlockHighlightType": "prettytserr"
}
